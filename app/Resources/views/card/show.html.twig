{% extends 'base.html.twig' %}

{% block body %}
    <h1>Card</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ card.id }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ card.picture }}</td>
            </tr>
            <tr>
                <th>Nbcard</th>
                <td>{{ card.nbcard }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('card_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
